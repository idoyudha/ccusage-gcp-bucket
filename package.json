{
  "name": "ccusage-gcp-bucket",
  "version": "1.0.0",
  "description": "simple npm package that extending claude code usage, upload daily to google cloud bucket",
  "main": "index.js",
  "bin": {
    "ccusage-gcp": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "upload-now": "node index.js --now",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "echo 'Run npm run setup-startup to enable automatic startup'",
    "setup-startup": "./scripts/install-service.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idoyudha/ccusage-gcp-bucket.git"
  },
  "author": "Ido Yudhatama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/idoyudha/ccusage-gcp-bucket/issues"
  },
  "homepage": "https://github.com/idoyudha/ccusage-gcp-bucket#readme",
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "ccusage": "^15.5.1",
    "dotenv": "^17.2.0",
    "node-cron": "^4.2.1"
  },
  "keywords": ["claude", "usage", "monitoring", "google-cloud", "storage", "scheduler"]
}
